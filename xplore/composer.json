{
    "name": "phpxplore/framework",
    "description": "Custom PHP Framework.",
    "keywords": ["framework", "phpxplore"],
    "minimum-stability": "dev",
    "license": "MIT",
    "authors": [
        {
            "name": "Nauman Bashir",
            "email": "naumanbashir.dev@gmail.com"
        }
